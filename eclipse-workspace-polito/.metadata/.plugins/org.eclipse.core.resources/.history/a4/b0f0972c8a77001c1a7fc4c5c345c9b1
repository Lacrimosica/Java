
public class X extends Thread {
	
	
	String name;
	int number;
	
	public X(String x, int xx) {
		this.name = x;
		this.number =xx;
	}
	public X(String x, int xx) {
		for(int i= xx ; i>0 ; i--) {
			System.out.println(x + " : " + i);
		}
	}
	public void run() {
		System.out.println("hello world X");
	}
	

}
