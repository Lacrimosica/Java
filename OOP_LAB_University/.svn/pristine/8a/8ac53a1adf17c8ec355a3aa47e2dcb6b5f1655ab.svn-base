package university;

public class Student {
	String name;
	int id;
	
	public Student(String name, int num){
		this.name = name;
		this.id = num + 10000;
	}
	
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	
}


