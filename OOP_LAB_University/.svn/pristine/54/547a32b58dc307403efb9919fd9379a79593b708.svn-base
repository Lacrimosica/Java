package university;

public class Student {
	private String name;
	private int id;
	private int n_enrollee;
	
	public Student() {
		System.out.println("new student class made");
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	
}


